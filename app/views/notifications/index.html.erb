<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <ul class="notification-index-list">
        <%= render @notifications %>
      </ul>
    </div>
  </div>
</div>
<% @notifications.each do |notification| %>
  <%= render 'notification', notification: notification %>
<% end %>

